    "com.android.freespeech": {
        "name": "FreeSpeech",
        "contexts": {
            "OS": [{
                    "id": "android",
                    "version": ">=0.1"
                }]
        },
        "settingsHandlers": {
            "configuration": {
                "type": "gpii.settingsHandlers.noSettings",
                "capabilities": [
                    "http://registry\\.gpii\\.net/common/screenReaderTTSEnabled"
                ]
            }
        },
        "start": [
            {
                "type": "gpii.androidActivityManager.startFreespeech"
            }
        ]
    },

