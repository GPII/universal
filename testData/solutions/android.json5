    "com.android.audioManager": {
        "name": "Android Audio Manager",
        "contexts": {
            "OS": [
                {
                    "id": "android",
                    "version": ">=0.1"
                }
            ]
        },
        "settingsHandlers": {
            "configuration": {
                "type": "gpii.androidAudioManager.volume",
                "liveness": "live",
                "supportedSettings": {
                    "STREAM_MUSIC": {},
                    "STREAM_SYSTEM": {}
                },
                "capabilitiesTransformations": {
                    "STREAM_MUSIC": {
                        "transform": {
                            "type": "fluid.transforms.round",
                            "input": {
                                "transform": {
                                    "type": "fluid.transforms.linearScale",
