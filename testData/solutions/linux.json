    "org.gnome.orca": {
        "name": "ORCA Screen Reader",
        "contexts": {
            "OS": [{
                "id": "linux",
                "version": ">=2.6.26"
            }]
        },
        "settingsHandlers": {
            "configuration": {
                "type": "gpii.orca",
                "options": {
                    "user": "${{userToken}}"
                },
                "capabilities": [
                    "http://registry\\.gpii\\.net/common/screenReaderTTSEnabled"
