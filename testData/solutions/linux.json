                                                "transform" : {
                                                    "type": "fluid.transforms.binaryOp",
                                                    "leftPath": "http://registry\\.gpii\\.net/common/speechRate",
                                                    "operator": "<",
                                                    "right": 170
                                                }
                                            },
                                            "true": 1.8,
                                            "false": 4.4
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "voices\\.default\\.average-pitch": {
                        "transform": {
                            "type": "fluid.transforms.linearScale",
