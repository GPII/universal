                    "Braille.UseScreenModelForBrailleInRichEdits": {
                        "title": "Use Screen Model for Braille in Rich Edits",
                        "description": "Whether to use the screen model when providing field prompts and text in Braille with rich edits.",
                        "enum": [
                            0,
                            1
                        ],
                        "enumLabels": [
                            "off",
                            "on"
                        ],
                        "default": 1
