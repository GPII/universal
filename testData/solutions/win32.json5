                        // (highContrast.enabled || highContrastTheme) && (highContrastTheme != "regular-contrast")
                        "value": {
                            "transform": {
                                "type": "fluid.transforms.binaryOp",
                                "left": {
                                    "transform": {
                                        "type": "fluid.transforms.binaryOp",
                                        "leftPath": "http://registry\\.gpii\\.net/common/highContrast/enabled",
