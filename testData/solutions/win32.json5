                            "enum": [
                                0,
                                1
                            ],
                            "enumLabels": [
                                "off",
                                "on"
                            ],
                            "default": 0
                        }
                    },
                    "Braille.BrailleVerbosity": {
                        "schema": {
                            "title": "Braille Verbosity",
