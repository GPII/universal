                            "enum": [
                                0,
                                1
                            ],
                            "enumLabels": [
                                "off",
                                "on"
                            ]
                        }
                    },
                    "HTML.FormFieldPromptOptions": {
                        "schema": {
                            "title": "Form Field Prompt Options",
                            "description": "Which attributes to prefer when describing form fields.",
                            "enum": [
