                    // ------------- Basic Settings - Tutor Messages ----------
                    "OutputModes.TUTOR": {
                        "schema": {
                            "title": "Tutor messages",
                            "description": "Select which kind of menu and control help to announce",
                            "enum": [
                                "0|0|0|Tutor",
                                "1|1|1|Tutor",
                                "2|2|2|Tutor"
                            ],
                            "enumLabels": [
                                "Turn off menu and control help",
                                "Announce menu and control help",
                                "Announce custom messages only"
                            ],
                            "default": "1|1|1|Tutor"
                        }
                    },

                    // ------------- Basic Settings - Access Key --------------

                    "OutputModes.ACCESS_KEY": {
                        "schema": {
                            "title": "Access Key",
                            "description": "Select which access keys to speak",
                            "enum": [
                                "0|0|0|Access Key",
                                "1|1|1|Access Key",
                                "2|2|2|Access Key",
                                "3|3|3|Access Key"
                            ],
                            "enumLabels": [
                                "Off",
                                "Speak all",
                                "Speak menus only",
                                "Speak dialogs only"
                            ],
                            "default": "1|1|1|Access Key"
                        }
                    },

