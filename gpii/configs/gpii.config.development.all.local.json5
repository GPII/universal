/**
 * This configuration runs the entire system locally and in development mode.
 *
 * This mean that:
 * * A all-in-one configuration with gpii.config.development.local.json5 (See corresponding file for details)
 * * Pouch Manager to store data on the local pouchDB
 */
{
    "type": "gpii.config.development.all.local",
    "mergeConfigs": [
        "%gpii-universal/gpii/configs/gpii.config.development.local.json5",
        "%pouchManager/configs/gpii.pouchManager.config.base.json5"
    ]
}
