                    "armenian": "hy" ,
                    "armenian-west": "hy-arevmda" ,
                    "indonesian-test": "id" ,
                    "icelandic-test": "is" ,
                    "italian": "it" ,
                    "lojban": "jbo" ,
                    "georgian-test": "ka" ,
