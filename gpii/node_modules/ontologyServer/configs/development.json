{
    "typeName": "ontologyServer.development",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "components": {
            "server": {
                "type": "kettle.server",
                "options": {
                    "logging": true,
                    "components": {
                        "ontologyServer": {
                            "type": "gpii.ontologyServer",
                            "options": {
                                "ontologySourceUrl": "file://%root/../../../testData/ontologies/%name.json"
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [{
        "demandingName": "kettle.urlExpander",
        "contextNames": "gpii.ontologyServer",
        "demandSpec": {
            "funcName": "kettle.urlExpander.development"
        }
    }, {
        "demandingName": "rawOntologySource",
        "contextNames": "gpii.ontologyServer",
        "demandSpec": {
            "funcName": "kettle.dataSource.URL"
        }
    }],
    "includes": [
        "./base.json"
    ]
}