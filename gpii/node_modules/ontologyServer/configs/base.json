{
    "demands": [{
        "demandingName": "kettle.requests.request.handler",
        "contextNames": "ontologyConvertPost",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "kettle.requests.request.handler.ontologyConvertPost",
                        "args": ["{that}", "{requestProxy}", "{request}.req.params.to", "{request}.req.body.preferences", "{ontologyServer}.ontologySource"]
                    }
                }
            }
        }
    }, {
        "demandingName": "ontologySource",
        "contextNames": "gpii.ontologyServer",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawOntologySource}"
                }
            }
        }
    }],
    "modules": [
        "ontologyServer"
    ]
}