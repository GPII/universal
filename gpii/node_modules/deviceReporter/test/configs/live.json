{
    "type": "gpii.tests.deviceReporter.config.live",
    "options": {
        "components": {
          "deviceReporterMock": {
              "type": "gpii.test.integration.deviceReporterAware",
              "options": {
                  "mockDeviceReporters": {
                      "gpii.packageKit.find": {
                          "mockFunc": "findNameInList",
                          "defaults": {
                              "gradeNames": "fluid.function",
                              "argumentMap": {
                                  "name": 0
                              }
                          }
                      }
                  },
                  "deviceReporters": {
                      "gpii.packageKit.find": {
                          "expectInstalled": [
                              "rainbowIslands",
                              "barbarian",
                              "greenBeret"
                          ]
                      }
                  }
              }
          }
        },
        "distributeOptions": {
            "deviceReporterType": {
                "record": "gpii.deviceReporter.live",
                "target": "{that gpii.deviceReporter}.options.gradeNames"
            },
            "platformReporter": {
                "record": {
                    "type": "gpii.test.integration.mockPlatformReporter",
                    "options": {
                        "platform": {
                            "id": "commodore",
                            "version": 64
                        }
                    }
                },
                "target": "{that deviceReporter platformReporter}"
            },
            "solutionsRegistry": {
                "record": {
                    "type": "kettle.dataSource.file",
                    "options": {
                        "gradeNames": "kettle.dataSource.file.moduleTerms",
                        "path": "%deviceReporter/test/data/live_solutionsRegistry.json"
                    }
                },
                "target": "{that deviceReporter}.options.components.solutionsRegistryDataSource"
            },
            "deviceReporterMocks": {
                "record": {
                  "funcName": null,
                  "func": "{deviceReporterMock}.resolveName"
                },
                "target": "{that deviceReporter > nameResolver}.options.invokers.resolveName"
            }
        }
    },
    "mergeConfigs": [
        "../../configs/gpii.deviceReporter.config.base.json"
    ]
}
