{
    "type": "gpii.tests.deviceReporter.config.static",
    "options": {
        "distributeOptions": {
            "development.installedSolutionsPath": {
                "record": "%deviceReporter/test/data/static_installedSolutions.json",
                "target": "{that deviceReporter installedSolutionsDataSource}.options.path"
            },
            "deviceReporterType": {
                "record": "gpii.deviceReporter.static",
                "target": "{that gpii.deviceReporter}.options.gradeNames"
            },
            "platformReporterType": {
                "record": {
                    "type": "gpii.test.integration.mockPlatformReporter",
                    "options": {
                        "platform": {
                            "id": "commodore",
                            "version": 64
                        }
                    }
                },
                "target": "{that deviceReporter platformReporter}"
            }
        }
    },
    "mergeConfigs": "../../configs/gpii.deviceReporter.config.base.json"
}
