{
    "type": "gpii.iod.config.local.base",
    "options": {
        "distributeOptions": {
            "packageData.local": {
                "record": {
                    "gradeNames": "kettle.dataSource.file",
                    "path": "%gpii-universal/testData/installOnDemand/%packageName.json",
                    "termMap": {
                        "packageName": "%packageName"
                    }
                },
                "target": "{that iod packageDataFallback}.options"
            },
            "packageData.moduleTerms": {
                "record": "kettle.dataSource.file.moduleTerms",
                "target": "{that iod packageDataFallback}.options.gradeNames"
            }
        }
    },
    "mergeConfigs": [
        "./gpii.iod.config.base.json"
    ]
}
