{
    "typeName": "solutionsRegistry.production",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "components": {
            "server": {
                "type": "kettle.server",
                "options": {
                    "logging": true,
                    "components": {
                        "solutionsRegistry": {
                            "type": "gpii.solutionsRegistry",
                            "options": {
                                "solutionRegistryUrl": "file://%root/../../../testData/solutions/%id.json",
                                "solutionQueryUrl": "file://%root/../../../testData/solutions/%os.json"
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [{
        "demandingName": "kettle.urlExpander",
        "contextNames": "gpii.solutionsRegistry",
        "demandSpec": {
            "funcName": "kettle.urlExpander.development"
        }
    }, {
        "demandingName": "rawSolutionSource",
        "contextNames": "gpii.solutionsRegistry",
        "demandSpec": {
            "funcName": "kettle.dataSource.URL"
        }
    }, {
        "demandingName": "rawSolutionQuerySource",
        "contextNames": "gpii.solutionsRegistry",
        "demandSpec": {
            "funcName": "kettle.dataSource.URL"
        }
    }],
    "includes": [
        "./base.json"
    ]
}