                  "left": 21.753,
                  "operator": "-",
                  "right": {
                    "transform": {
                      "type": "fluid.transforms.binaryOp",
                      "left": {
                        "transform": {
                          "type": "fluid.transforms.binaryOp",
                          "left": 0.0317,
                          "operator": "*",
