{
  "name": "Windows 10 system volume",
  "settingsHandlers": {
    "configure": {
      "supportedSettings": {
        "Volume": {
          "schema": {
            "title": "System volume",
            "description": "Changes the current system volume.",
            "type": "object",
            "properties": {
              "value": {
                "type": "number",
                "default": 0.5,
                "minimum": 0,
                "maximum": 1,
                "multipleOf": 0.01
              }
            }
          }
        }
      },
      "type": "gpii.windows.nativeSettingsHandler",
      "liveness": "live",
      "options": {
        "functionName": "Volume"
      }
    }
  },
  "contexts": {
    "OS": [
      {
        "windows": {
          "id": "win32",
          "version": ">=5.0"
        }
      }
    ]
  },
  "isInstalled": [
    {
      "type": "gpii.deviceReporter.alwaysInstalled"
    }
  ],
  "start": [],
  "stop": []
}
