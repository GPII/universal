{
    "name": "Windows DPI",
    "settingsHandlers": {
        "configure": {
            "supportedSettings": {
                "screen-dpi": {
                    "schema": {
                        "title": "Screen DPI",
                        "description": "Screen DPI of the default display.",
                        "type": "number",
                        "minimum": -3,
                        "maximum": 5
                    }
                }
            },
            "type": "gpii.windows.displaySettingsHandler",
            "liveness": "live"
        }
    },
    "contexts": {
        "OS": [
            {
                "windows": {
                    "id": "win32",
                    "version": ">=5.0"
                }
            }
        ]
    },
    "isInstalled": [
        {
            "type": "gpii.deviceReporter.alwaysInstalled"
        }
    ]
}
