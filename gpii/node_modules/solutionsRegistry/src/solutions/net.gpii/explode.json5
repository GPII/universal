{
  "name": "GPII Test solution for triggering settingsHandler errors",
  "launchHandlers": {
    "launch": {
      "type": "gpii.settingsHandlers.exploding",
      "options": {
        "launchHandler": true,
        "preferences": "${{session}.preferences}"
      }
    }
  },
  "isRunning": [
    "launchers.launch"
  ],
  "isInstalled": [
    {
      "type": "gpii.deviceReporter.alwaysInstalled"
    }
  ],
  "parents": [
    "gpii.solutionsRegistry.solution",
    "gpii.solutionsRegistry.context.windows"
  ],
  "key": "net.gpii.explode",
  "settingsHandlers": {
    "configure": {
      "liveness": "live",
      "settingsHandlerNamespace": "gpii.settingsHandlers.exploding",
      "key": "configure",
      "supportedSettings": {
        "explodeMethod": {
          "schema": {
            "title": "Explode Method",
            "description": "The strategy to be used when exploding",
            "enum": [
              "reject",
              "fail",
              "throw"
            ],
            "enumLabels": [
              "reject",
              "fail",
              "throw"
            ]
          },
          "key": "explodeMethod"
        },
        "explodeOn": {
          "schema": {
            "title": "Explode On",
            "description": "Whether to explode during the get or set settingsHandler method",
            "enum": [
              "get",
              "set",
              "launch",
              "stop"
            ],
            "enumLabels": [
              "get",
              "set",
              "launch",
              "stop"
            ]
          },
          "key": "explodeOn"
        }
      }
    }
  }
}
