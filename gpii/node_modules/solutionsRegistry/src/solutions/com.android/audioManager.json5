{
  "name": "Android Audio Manager",
  "parents": [
    "gpii.solutionsRegistry.solution",
    "gpii.solutionsRegistry.context.android"
  ],
  "key": "com.android.audioManager",
  "settingsHandlers": {
    "configuration": {
      "liveness": "live",
      "capabilitiesTransformations": {
        "STREAM_MUSIC": {
          "transform": {
            "type": "fluid.transforms.round",
            "input": {
              "transform": {
                "type": "fluid.transforms.linearScale",
                "inputPath": "http://registry\\.gpii\\.net/common/volumeTTS",
                "factor": 15
              }
            }
          }
        },
        "STREAM_SYSTEM": {
          "transform": {
            "type": "fluid.transforms.round",
            "input": {
              "transform": {
                "type": "fluid.transforms.linearScale",
                "inputPath": "http://registry\\.gpii\\.net/common/volume",
                "factor": 7
              }
            }
          }
        }
      },
      "inverseCapabilitiesTransformations": {},
      "settingsHandlerNamespace": "gpii.androidAudioManager.volume",
      "key": "configuration",
      "supportedSettings": {
        "STREAM_MUSIC": {
          "schema": {
            "title": "Music Volume",
            "description": "Music volume.",
            "type": "number"
          },
          "key": "STREAM_MUSIC"
        },
        "STREAM_SYSTEM": {
          "schema": {
            "title": "System Volume",
            "description": "System volume.",
            "type": "number"
          },
          "key": "STREAM_SYSTEM"
        }
      }
    }
  }
}
