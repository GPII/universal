{
  "name": "Android UI Settings.System",
  "parents": ["gpii.solutionsRegistry.solution", "gpii.solutionsRegistry.context.android"],
  "settingsHandlers": {
    "configuration": {
      "liveness": "live",
      "capabilitiesTransformations": {
        "dim_screen": {
          "transform": {
            "type": "fluid.transforms.condition",
            "conditionPath": "http://registry\\.gpii\\.net/common/screenDim",
            "true": 1,
            "false": 0
          }
        },
        "haptic_feedback_enabled": {
          "transform": {
            "type": "fluid.transforms.condition",
            "conditionPath": "http://registry\\.gpii\\.net/common/hapticFeedback",
            "true": 1,
            "false": 0
          }
        },
        "accelerometer_rotation": {
          "transform": {
            "type": "fluid.transforms.condition",
            "conditionPath": "http://registry\\.gpii\\.net/common/screenRotation",
            "true": 1,
            "false": 0
          }
        },
        "user_rotation": "http://registry\\.gpii\\.net/common/screenDefaultRotation",
        "screen_off_timeout": {
          "transform": {
            "type": "fluid.transforms.linearScale",
            "inputPath": "http://registry\\.gpii\\.net/common/screenOffTime",
            "factor": 1000
          }
        }
      },
      "inverseCapabilitiesTransformations": {},
      "settingsHandlerOptions": {
        "settingType": "System"
      },
      "settingsHandlerNamespace": "gpii.androidSettings",
      "supportedSettings": {
        "dim_screen": {},
        "haptic_feedback_enabled": {},
        "accelerometer_rotation": {},
        "user_rotation": {},
        "screen_off_timeout": {}
      }
    }
  }
}
