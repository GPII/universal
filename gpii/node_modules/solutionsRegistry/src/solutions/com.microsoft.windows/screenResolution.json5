{
  "name": "Windows Screen Resolution",
  "isInstalled": [
    {
      "type": "gpii.deviceReporter.alwaysInstalled"
    }
  ],
  "parents": [
    "gpii.solutionsRegistry.solution",
    "gpii.solutionsRegistry.context.windows"
  ],
  "key": "com.microsoft.windows.screenResolution",
  "settingsHandlers": {
    "configuration": {
      "liveness": "liveRestart",
      "settingsHandlerNamespace": "gpii.windows.displaySettingsHandler",
      "key": "configuration",
      "supportedSettings": {
        "screen-resolution": {
          "schema": {
            "title": "Screen resolution",
            "description": "Screen resolution of the default display.",
            "type": "object",
            "properties": {
              "width": {
                "type": "integer",
                "required": true
              },
              "height": {
                "type": "integer",
                "required": true
              },
              "additionalProperties": false
            }
          },
          "key": "screen-resolution"
        }
      }
    }
  }
}
