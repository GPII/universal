{
  "name": "Windows DPI",
  "parents": ["gpii.solutionsRegistry.solution", "gpii.solutionsRegistry.context.windows"],
  "isInstalled": [
    {
      "type": "gpii.deviceReporter.alwaysInstalled"
    }
  ],
  "settingsHandlers": {
    "configure": {
      "liveness": "live",
      "capabilitiesTransformations": {
        "screen-dpi": "http://registry\\.gpii\\.net/common/DPIScale"
      },
      "settingsHandlerNamespace": "gpii.windows.displaySettingsHandler",
      "supportedSettings": {
        "screen-dpi": {
          "schema": {
            "title": "Screen DPI",
            "description": "Screen DPI of the default display.",
            "type": "number",
            "minimum": 1,
            "maximum": 5
          }
        }
      }
    }
  }
}
