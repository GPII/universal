{
  "name": "Windows DPI",
  "isInstalled": [
    {
      "type": "gpii.deviceReporter.alwaysInstalled"
    }
  ],
  "parents": [
    "gpii.solutionsRegistry.solution",
    "gpii.solutionsRegistry.context.windows"
  ],
  "key": "com.microsoft.windows.screenDPI",
  "settingsHandlers": {
    "configure": {
      "liveness": "live",
      "capabilitiesTransformations": {
        "screen-dpi": {
          "transform": {
            "type": "fluid.transforms.round",
            "input": {
              "transform": {
                "type": "fluid.transforms.binaryOp",
                "left": {
                  "transform": {
                    "type": "fluid.transforms.binaryOp",
                    "left": 5,
                    "right": 7,
                    "operator": "/"
                  }
                },
                "right": {
                  "transform": {
                    "type": "fluid.transforms.binaryOp",
                    "leftPath": "http://registry\\.gpii\\.net/common/DPIScale",
                    "right": 3,
                    "operator": "+"
                  }
                },
                "operator": "*"
              }
            }
          }
        }
      },
      "settingsHandlerNamespace": "gpii.windows.displaySettingsHandler",
      "key": "configure",
      "supportedSettings": {
        "screen-dpi": {
          "schema": {
            "title": "Screen DPI",
            "description": "Screen DPI of the default display.",
            "type": "number",
            "minimum": 1,
            "maximum": 5
          },
          "key": "screen-dpi"
        }
      }
    }
  }
}
