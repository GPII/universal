{
    "demands": [{
        "demandingName": "kettle.requests.request.handler",
        "contextNames": "userLogin",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "gpii.request.flowManager.onUserLogin",
                        "args": ["{request}.req.params.token", "{that}"],
                        "dynamic": true
                    }
                },
                "gradeNames": ["gpii.request.flowManager.userLogin"]
            }
        }
    }, {
        "demandingName": "kettle.requests.request.handler",
        "contextNames": "userLogout",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "gpii.request.flowManager.onUserLogout",
                        "args": ["{requestProxy}", "{request}.req.params.token", "{flowManager}.lifecycleManagerServerDataSource"],
                        "dynamic": true
                    }
                }
            }
        }
    }, {
        "demandingName": "rawPreferencesDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "funcName": "kettle.dataSource.URL",
            "options": {
                "url": "{gpii.flowManager}.options.urls.preferences",
                "termMap": {
                    "token": "%token"
                }
            }
        }
    }, {
        "demandingName": "preferencesDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawPreferencesDataSource}"
                }
            }
        }
    }, {
        "demandingName": "rawDeviceReporterDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "funcName": "kettle.dataSource.URL",
            "options": {
                "url": "{gpii.flowManager}.options.urls.deviceReporter"
            }
        }
    }, {
        "demandingName": "deviceReporterDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawDeviceReporterDataSource}"
                }
            }
        }
    }, {
        "demandingName": "rawMatchMakerDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "funcName": "kettle.dataSource.URL",
            "options": {
                "url": "{gpii.flowManager}.options.urls.matchMaker",
                "writable": true
            }
        }
    }, {
        "demandingName": "matchMakerDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawMatchMakerDataSource}"
                }
            }
        }
    }, {
        "demandingName": "rawLifecycleManagerServerDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "funcName": "kettle.dataSource.URL",
            "options": {
                "url": "{gpii.flowManager}.options.urls.lifecycleManagerServer",
                "writable": true,
                "termMap": {
                    "operation": "%operation",
                    "token": "%token"
                }
            }
        }
    }, {
        "demandingName": "lifecycleManagerServerDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawLifecycleManagerServerDataSource}"
                }
            }
        }
    }],
    "modules": [
        "flowManager"
    ]
}