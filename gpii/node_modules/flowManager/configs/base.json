{
    "typeName": "flowManager.base",
    "options": {
        "components": {
            "server": {
                "type": "kettle.server",
                "options": {
                    "port": 8081,
                    "components": {
                        "flowManager": {
                            "type": "gpii.flowManager"
                        }
                    }
                }
            }
        },
        "distributeOptions": [{
            "source": "{that}.options.flowManagerBaseGrades",
            "target": "{that flowManager}.options.gradeNames"
        }],
        "flowManagerBaseGrades": ["gpii.flowManager.base"]
    },
    "modules": [
        "flowManager"
    ]
}
