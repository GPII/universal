{
    "typeName": "flowManager.cloudBased",
    "options": {
        "gradeNames": ["fluid.littleComponent", "autoInit"],
        "distributeOptions": [{
            "source": "{that}.options.flowManagerCloudBasedServerGrades",
            "target": "{that server}.options.gradeNames"
        }, {
            "source": "{that}.options.flowManagerCloudBasedGrades",
            "target": "{that flowManager}.options.gradeNames"
        }, {
            "source": "{that}.options.cloudBasedUrls",
            "target": "{that flowManager}.options.urls"
        }, {
            "source": "{that}.options.cloudBasedLogging",
            "target": "{that server}.options.logging"
        }, {
            "source": "{that}.options.matchMakers",
            "target": "{that flowManager}.options.matchMakers"
        }, {
            "source": "{that}.options.matchMakerDispatcherDisabled",
            "target": "{that flowManager}.options.matchMakerDispatcherDisabled"
        }],
        "cloudBasedUrls": {
            "preferences": "http://localhost:8081/preferences/%userToken",
            "deviceReporter": "",
            "solutionsRegistry": "file://%root/../../../testData/solutions/%os.json"
        },
        "matchMakers": {
            "flat": {
                "url": "http://localhost:8081"
            },
            "RuleBased": {
                "url": "http://localhost:8078"
            }
        },
        "matchMakerDispatcherDisabled": true,
        "cloudBasedLogging": true,
        "flowManagerCloudBasedGrades": ["kettle.urlExpander.distributeDevVariables", "gpii.flowManager.cloudBased", "gpii.flowManager.cloudBased.oauth2"],
        "flowManagerCloudBasedServerGrades": ["kettle.use.CORS"]
    },
    "includes": [
        "./production.json"
    ]
}
