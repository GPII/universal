{
    "type": "gpii.flowManager.untrusted.config.development",
    "options": {
        "gradeNames": "fluid.component",
        "distributeOptions": {
            "flowManager.untrustedGrades": {
                "record": "gpii.flowManager.untrusted",
                "target": "{that flowManager}.options.gradeNames"
             },
            "flowManager.untrustedSettingsGetUrl": {
                "record": "http://localhost:8084/%userToken/untrusted-settings/%device",
                "target": "{that flowManager untrustedSettingsDataSource}.options.untrustedSettingsGetUrl"
            },
            "flowManager.untrustedSettingsPutUrl": {
                "record": "http://localhost:8084/%userToken/untrusted-settings",
                "target": "{that flowManager untrustedSettingsDataSource}.options.untrustedSettingsPutUrl"
            },
            "flowManager.accessTokenUrl": {
                "record": "http://localhost:8084/access_token",
                "target": "{that flowManager untrustedSettingsDataSource}.options.accessTokenUrl"
            },
            "flowManager.clientCredentialFilePath": {
                "record": "%gpii-universal/testData/clientCredentials/pilot.json",
                "target": "{that flowManager untrustedSettingsDataSource}.options.clientCredentialFilePath"
            }
        }
    },
    "mergeConfigs": [
        "./gpii.flowManager.config.development.json"
    ]
}
