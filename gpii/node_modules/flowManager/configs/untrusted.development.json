{
    "typeName": "flowManager.untrusted",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "distributeOptions": [{
            "source": "{that}.options.untrustedFlowManagerGrades",
            "target": "{that flowManager}.options.gradeNames"
        }, {
            "source": "{that}.options.untrustedFlowManagerUrls",
            "target": "{that flowManager}.options.urls"
        }],
        "untrustedFlowManagerGrades": ["kettle.urlExpander.distributeDevVariables", "gpii.flowManager.untrusted"],
        "untrustedFlowManagerUrls": {
            "deviceReporter": "http://localhost:%port/device",
            "settings": "http://localhost:8088/%userToken/untrusted-settings/%device",
            "solutionsRegistry": "file://%root/../../../testData/solutions/%os.json"
        }
    },
    "includes": [
        "./base.json",
        "./io.json"
    ]
}
