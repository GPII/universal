{
    "type": "gpii.flowManager.cloudBased.config.development",
    "options": {
        "gradeNames": "fluid.component",
        "distributeOptions": {
            "flowManager.cloudGrades": {
                "record": ["gpii.flowManager.cloudBased", "gpii.flowManager.cloudBased.oauth2"],
                "target": "{that flowManager}.options.gradeNames"
            },
            "flowManager.authzPrefsServer": {
                "record": "http://localhost:8081/preferences?view=%view",
                "target": "{that gpii.oauth2.authorizationService}.options.urls.preferencesPost"
            }
        }
    },
    "mergeConfigs": [
        "./gpii.flowManager.config.development.json"
    ]
}
