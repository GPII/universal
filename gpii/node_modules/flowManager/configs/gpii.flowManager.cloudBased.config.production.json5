/**
 * This config is included by `gpii.config.cloudBased.flowManager.production` to start Flow Manager in the production mode.
 */
{
    "type": "gpii.flowManager.cloudBased.config.production",
    "options": {
        "gradeNames": "fluid.component",
        "distributeOptions": {
            "flowManager.cloudGrades": {
                "record": ["gpii.flowManager.cloudBased", "gpii.flowManager.cloudBased.oauth2"],
                "target": "{that flowManager}.options.gradeNames"
            }
        }
    },
    "mergeConfigs": [
        "./gpii.flowManager.config.production.json5"
    ]
}
