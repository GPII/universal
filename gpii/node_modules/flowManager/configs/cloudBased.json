{
    "typeName": "flowManager.cloudBased",
    "options": {
        "components": {
            "server": {
                "options": {
                    "components": {
                        "flowManager": {
                            "type": "gpii.flowManager.cloudBased",
                            "options": {
                                "typeName": "flowManager.cloudBased",
                                "urls": {
                                    "preferences": "http://preferences.gpii.net/user/%token",
                                    "lifecycleManagerServer": "",
                                    "deviceReporter": "",
                                    "matchMaker": "http://localhost:8080/match"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [
        {
            "demandingName": "kettle.requests.request.handler",
            "contextNames": ["userLogin", "gpii.flowManager.cloudBased"],
            "demandSpec": {
                "options": {
                    "invokers": {
                        "handle": {
                            "funcName": "gpii.request.flowManager.unsupported",
                            "args": "{requestProxy}.events.onError"
                        }
                    }
                }
            }
        }, {
            "demandingName": "kettle.requests.request.handler",
            "contextNames": ["userLogout", "gpii.flowManager.cloudBased"],
            "demandSpec": {
                "options": {
                    "invokers": {
                        "handle": {
                            "funcName": "gpii.request.flowManager.unsupported",
                            "args": "{requestProxy}.events.onError"
                        }
                    }
                }
            }
        },
        {
            "demandingName": "kettle.requests.request.handler",
            "contextNames": "settings",
            "demandSpec": {
                "options": {
                    "invokers": {
                        "handle": {
                            "funcName": "gpii.request.flowManager.onSettings",
                            "args": ["{request}.req.params.token", "{that}", "{request}.req.params.device"]
                        }
                    },
                    "gradeNames": ["gpii.request.flowManager.settings"]
                }
            }
        },
        {
            "demandingName": "rawLifecycleManagerServerDataSource",
            "contextNames": ["gpii.flowManager", "gpii.flowManager.cloudBased"],
            "demandSpec": {
                "funcName": "fluid.emptySubcomponent"
            }
        },
        {
            "demandingName": "lifecycleManagerServerDataSource",
            "contextNames": ["gpii.flowManager", "gpii.flowManager.cloudBased"],
            "demandSpec": {
                "funcName": "fluid.emptySubcomponent"
            }
        },
        {
            "demandingName": "rawDeviceReporterDataSource",
            "contextNames": ["gpii.flowManager", "gpii.flowManager.cloudBased"],
            "demandSpec": {
                "funcName": "fluid.emptySubcomponent"
            }
        },
        {
            "demandingName": "deviceReporterDataSource",
            "contextNames": ["gpii.flowManager", "gpii.flowManager.cloudBased"],
            "demandSpec": {
                "funcName": "fluid.emptySubcomponent"
            }
        }
    ],
    "includes": [
        "./production.json"
    ]
}
