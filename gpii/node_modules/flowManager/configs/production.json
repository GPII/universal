{
    "typeName": "flowManager.production",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "distributeOptions": [{
            "source": "{that}.options.productionLogging",
            "target": "{that server}.options.logging"
        }, {
            "source": "{that}.options.productionUrls",
            "target": "{that flowManager}.options.urls"
        }],
        "productionUrls": {
            "preferences": "http://preferences.gpii.net/preferences/%userToken",
            "deviceReporter": "http://localhost:%port/device",
            "matchMaker": "http://localhost:%port/match"
        },
        "productionLogging": false
    },
    "includes": [
        "./base.json"
    ]
}