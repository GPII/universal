{
    "typeName": "update",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "distributeOptions": [{
            "source": "{that}.options.port",
            "target": "{that server}.options.port"
        }, {
            "source": "{that}.options.solutionsRegistryUrl",
            "target": "{that flowManager}.options.urls.solutionsRegistry"
        }, {
            "source": "{that}.options.rawPreferencesSourceUrl",
            "target": "{that rawPreferencesServer}.options.rawPreferencesSourceUrl"
        }, {
            "source": "{that}.options.installedSolutionsUrl",
            "target": "{that deviceReporter}.options.installedSolutionsUrl"
        }],
        "rawPreferencesSourceUrl": "file://%root/../flowManager/test/data/%userToken.json",
        "solutionsRegistryUrl": "file://%root/../flowManager/test/data/update_solutionsRegistry.json",
        "installedSolutionsUrl": "file://%root/../flowManager/test/data/update_deviceReporter.json",
        "port": 8081
    },
    "includes": [
        "../../configs/io.json",
        "../../configs/development.json",
        "../../../deviceReporter/configs/development.json",
        "../../../flatMatchMaker/configs/development.all.local.json",
        "../../../preferencesServer/configs/development.json",
        "../../../rawPreferencesServer/configs/development.json",
        "../../../solutionsRegistry/configs/development.json"
    ]
}
