{
    "languageAssistance": {
        "applications": [{
            "name": "Read Write Gold",
            "id": "com.texthelp.readWriteGold",
            "priority": 100,
            "parameters": {
                "ApplicationSettings.AppBar.Width": 1072,
                "ApplicationSettings.AppBar.ShowText": true,
                "ApplicationSettings.AppBar.optToolbarShowText": true,
                "ApplicationSettings.AppBar.LargeIcons": true,
                "ApplicationSettings.AppBar.optToolbarLargeIcons": true,
                "ApplicationSettings.Speech.optSAPI5Speed": 50,
                "ApplicationSettings.Speech.optAutoUseScreenReading": false
            }
        }]
    },
    "display": {
