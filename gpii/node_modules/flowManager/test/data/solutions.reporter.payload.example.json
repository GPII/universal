[
    {
        "name": "UI Options",
        "id": "fluid.uiOptions",
        "contexts": {             
            "OS": [{
                "id": "linux",
                "version": ">=2.6.26"
            }, {
                "id": "win32",
                "version": ">=5.0"
            }]
        },
        "settingsHandlers": [
            {
                "type": "gpii.settingsHandlers.EmptyHandler.set",
                "capabilities": ["display"]
            }
        ],
        "lifecycleManager": {
            "start": [
                {
                    "type": "gpii.launch.exec",
                    "command": "firefox http://ec2-107-21-143-113.compute-1.amazonaws.com:8080/demos/Mammals.html?token={{userToken}}"
                }
            ],
            "stop": [
                {
                    "type": "gpii.launch.exec",
                    "command": "pkill firefox"
                }
            ]
        }
    }, {
