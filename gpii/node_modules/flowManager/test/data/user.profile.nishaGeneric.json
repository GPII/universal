{
    "control": {
        "onscreenKeyboard": {
            "applications": [{
                "name": "GNOME Shell On Screen Keyboard",
                "id": "org.gnome.desktop.a11y.keyboard",
                "priority": 100
            }, {
                "name": "Windows Built-in OnScreen Keyboard",
                "id": "com.microsoft.windows.onscreenKeyboard",
                "priority": 100
            }]
        },
        "mouseEmulation": {
            "-provisional-initDelay": 0.120,
            "cursorSpeed": 0.850,
            "cursorAcceleration": 0.800,
            "-provisional-mouseEmulationEnabled": true
        },
        "keyboardEnhancement": {
            "stickyKeys": true,
            "slowKeys": {
                "-provisional-slowKeysEnable": true,
                "slowKeysInterval": 0.4
            },
            "debounceKeys": {
                "debounceInterval": 0.20,
                "-provisional-debounceEnable": true
            },
            "applications": [{
                "name": "GNOME Shell Keyboard Settings",
                "id": "org.gnome.desktop.a11y.keyboard",
                "priority": 100
            }]
        },
        "structuralNavigation": {
            "tableOfContents": false
        }
    }
}