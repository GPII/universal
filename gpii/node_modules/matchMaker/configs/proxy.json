{
    "typeName": "matchMakerProxy",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "components": {
            "server": {
                "type": "kettle.server",
                "options": {
                    "port": 8083,
                    "logging": true,
                    "components": {
                        "matchMakerProxy": {
                            "type": "gpii.matchMakerProxy",
                            "options": {
                                "matchMakerPathMap": {
                                    "flat": "http://localhost:8079",
                                    "ruleBased": "http://rbmm.gpii.net:80",
                                    "statistical": "http://stmm.gpii.net:80"
                                }
                            }
                        }
                    }
                }
            },
            "flatMatchMakerServer": {
                "type": "kettle.server",
                "options": {
                    "port": 8079,
                    "components": {
                        "matchMaker": {
                            "type": "gpii.matchMaker",
                            "options": {
                                "gradeNames": ["gpii.matchMaker.dev"],
                                "solutionsReporterUrl": "file://%root/../../../testData/solutions/%os.json",
                                "ontologyServerUrl": "file://%root/../../../testData/ontologies/ontologies.json"
                            }
                        }
                    }
                }
            }
        }
    },
    "includes": [
        "./base.json"
    ],
    "modules": [
        "ontologyServer"
    ]
}
