{
    "typeName": "matchMaker.production",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "components": {
            "server": {
                "type": "gpii.server",
                "options": {
                    "logging": false,
                    "components": {
                        "matchMaker": {
                            "type": "gpii.matchMaker",
                            "options": {
                                "solutionsReporterUrl": "http://localhost:%port/solution?os=%os",
                                "ontologyServerUrl": "http://localhost:%port/ontology/convert/ISO24751-2"
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [{
        "demandingName": "rawOntologyServer",
        "contextNames": "gpii.matchMaker",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL",
            "options": {
                "url": "{gpii.matchMaker}.options.ontologyServerUrl",
                "writable": true
            }
        }
    }],
    "includes": [
        "./base.json"
    ]
}