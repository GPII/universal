{
    "typeName": "ontologyServer.development",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "distributeOptions": [{
            "source": "{that}.options.devUrls",
            "target": "{that ontologyServer}.options.urls"
        }],
        "devUrls": {
            "rawPreferences": "http://localhost:%port/user/%token",
            "ontologyServer": "http://localhost:%port/user/%token"
        },
        "components": {
            "server": {
                "type": "kettle.server",
                "options": {
                    "logging": true,
                    "port": 8081,
                    "components": {
                        "ontologyServer": {
                            "type": "gpii.ontologyServer",
                            "options": {
                                "gradeNames": ["gpii.ontologyServer.dev"],
                                "ontologySourceList": "%root/../../../testData/ontologies",
                                "ontologySourceUrl": "file://%root/../../../testData/ontologies/%name.json"
                            }
                        }
                    }
                }
            }
        }
    },
    "includes": [
        "./base.json"
    ]
}